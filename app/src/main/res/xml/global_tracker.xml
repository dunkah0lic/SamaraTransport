<?xml version="1.0" encoding="utf-8" ?>
<resources>

    <!-- The application version. Defaults to android:versionName specified in the AndroidManifest.xml -->
    <string name="ga_appVersion">1.0</string>

    <!-- The dispatching period in seconds when Google Play services is
         unavailable. The default period is 1800 seconds or 30 minutes -->
    <integer name="ga_dispatchPeriod">1800</integer>

    <!-- Enable dry run mode. Default is false -->
    <bool name="ga_dryRun">false</bool>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!-- The screen names that will appear in reports -->
    <screenName name="com.nikolaychernov.samaratransport.ArrivalActivity">
        SamaraTransport ArrivalActivity
    </screenName>
    <screenName name="com.nikolaychernov.samaratransport.DirectionSelectActivity">
        SamaraTransport DirectionSelectActivity
    </screenName>
    <screenName name="com.nikolaychernov.samaratransport.MapsActivity">
        SamaraTransport MapsActivity
    </screenName>
    <screenName name="com.nikolaychernov.samaratransport.StopSearchActivity">
        SamaraTransport StopSearchActivity
    </screenName>
    <screenName name="com.nikolaychernov.samaratransport.SettingsActivity">
        SamaraTransport SettingsActivity
    </screenName>

</resources>